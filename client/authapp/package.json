{
  "name": "authapp",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "npm run build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "npm run build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "start:server": "nodemon server.js"
  },
  "dependencies": {
    "vue": "^3.2.33",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "core-js": "^3.22.7",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.4",
    "mongoose-unique-validator": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.20",
    "cypress": "^9.5.4",
    "jsdom": "^19.0.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
